#include<stdio.h>
main(){
	int i,j,a;
	printf("enter Number of shirts:\n");
	scanf("%d",&a);
	int arr[a][2];
	for(i=0;i<a;i++){
		printf("enter age for %d",i+1);
		scanf("%d",&arr[i][0]);
		printf("enter price for %d",i+1);
		scanf("%d",&arr[i][1]);
	
    }
    int temp;
    for(i=0;i<a;i++){
    	for(j=i+1;j<a;j++){
    		if(arr[i][0]>arr[i+1][0]){
    			temp=arr[i][0];
    			arr[i][0]=arr[i+1][0];
    			arr[i+1][0]=temp;
			}
		}
	}
	int temp2;
    for(i=0;i<a;i++){
    	for(j=i+1;j<a;j++){
    		if(arr[i][1]<arr[i+1][1]){
    			temp2=arr[i][1];
    			arr[i][1]=arr[i+1][1];
    			arr[i+1][1]=temp2;
			}
		}
	}
	for(i=0;i<a;i++){
	
	printf(" %d\t %d\n",arr[i][0],arr[i][1]);}
}
